fun main(args: Array<String>) {
    
    // Input name
    println("Enter Your Name: ")
    val name = readLine()
    
    // Input Address
    println("Enter Your Address: ")
    val add = readLine()
    
    // Input Country Name
    println("Enter Your Country Name : ")   
    val country = readLine()
    
    // Printing input values
    println("Name: $name")
    println("Address: $add")
    println("County: $country")
}
